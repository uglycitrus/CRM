/* drop down */


#horizontal_navbar_container
	{
	}

#horizontal_navbar
	{
	font-size: small;
	font-family: arial, helvetica, sans-serif;
	width:100%;
	list-style-type: none; 
	text-align: center; 
	}
		
#horizontal_navbar a
	{
	text-align: center;
	display:block;
	border: 1px solid #555;
	white-space:nowrap;
	margin:3px;
	padding: 0.3em;
	}
	
#horizontal_navbar a:link, #horizontal_navbar a:visited, #horizontal_navbar a:active	/* menu at rest */
	{
	color: white;
	background-color: royalblue;
	text-decoration:none;
	}
	
#horizontal_navbar a:hover						/* menu on mouse-over  */
	{
	color: white;
	background-color: cornflowerblue;
	text-decoration:none;
	}	
	
#horizontal_navbar a.top_parent, #horizontal_navbar a.top_parent:hover  /* attaches down-arrow to all top-parents */
	{
	background-image: url(navdown_white.gif);
	background-position: right center;
	background-repeat: no-repeat;
	}
	
#horizontal_navbar a.parent, #horizontal_navbar a.parent:hover 	/* attaches side-arrow to all parents */
	{
	background-image: url(nav_white.gif);
	background-position: right center;
	background-repeat: no-repeat;
	}

#horizontal_navbar ul
	{
	list-style:none;
	margin:0;
	padding:0;
	float:left;
	width:10em;	/* width of all menu boxes */
	/* NOTE: For adjustable menu boxes you can comment out the above width rule.
	However, you will have to add padding in the "#menh a" rule so that the menu boxes
	will have space on either side of the text -- try it */
	}

#horizontal_navbar li
	{
	position:relative;
	width:100%;
	min-height: 1px;		/* Sophie Dennis contribution for IE7 */
	vertical-align: bottom;		/* Sophie Dennis contribution for IE7 */
	}

#horizontal_navbar ul ul
	{
	position:absolute;
	z-index:500;
	top:auto;
	display:none;
	padding: 1em;
	margin:-1em 0 0 -1em;
	}

#horizontal_navbar ul ul ul
	{
	top:0;
	left:100%;
	}

div#horizontal_navbar li:hover
	{
	cursor:pointer;
	z-index:100;
	}

div#horizontal_navbar li:hover ul ul,
div#horizontal_navbar li li:hover ul ul,
div#horizontal_navbar li li li:hover ul ul,
div#horizontal_navbar li li li li:hover ul ul
{display:none;}

div#horizontal_navbar li:hover ul,
div#horizontal_navbar li li:hover ul,
div#horizontal_navbar li li li:hover ul,
div#horizontal_navbar li li li li:hover ul
{display:block;}

/* End CSS Drop Down Menu */



