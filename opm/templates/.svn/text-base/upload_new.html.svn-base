{% extends "project_list_base.html" %}

{% block top%}{% endblock %}

{% block content %}
	<div>
		<form method = "POST" enctype="multipart/form-data" action="">
			<div class="formSection">

				<p>
					{{form.file.errors}}
					<label for = "file">file :</label>
					{{form.file}}
				</p>

				<p>
					{{form.file_title.errors}}
					<label for = "file_title">file_title :</label>
					{{form.file_title}}
				</p>
			</div>
		{% block submit_button %}<input type = "submit" value = "save"/>{% endblock %}
		</form>
	</div>
{% endblock %}

{% block bottom %}{% endblock %}
