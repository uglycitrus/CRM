{% extends "project_list_base.html" %}

{% block content %}
	<div id="control_bar">
		<a href="{%url upload_new %}">UPLOAD FILE</a>
		<form method = "POST" action="">
			<label for = "search">Search: {{form.search}}</label>
			<input type = "submit" value = "search"/>
		</form>

	</div>
	<div id="list">
		<table>
			<tr>
				<th>title</th>
				<th>file</th>
			</tr>
		{% for i in list %}
			<tr>
				<td>{{i.file_title}}</td>
				<td><a target="new window" href="{% url upload_view i.file%}">{{i.file}}</a></td>
			</tr>
		{% endfor %}
		</table>
	</div>
{% endblock %}
